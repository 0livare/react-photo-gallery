{
  "name": "@zposten/photo-gallery",
  "description": "A strongly typed, customizable, React photo viewer with mobile gestures built in that looks great right out of the box.",
  "author": "Zach Olivare <zach@olivare.net>",
  "version": "0.3.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "http://www.github.com/zposten/photoswipe-react"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "pub": "mkdir -p dist && cp -f ../README.md dist/README.md && pnpm run build && yarn version && npm publish --access public && git push && git push --tags"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "photoswipe": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/photoswipe": "^4.1.2",
    "@types/react": "^17.0.38",
    "cross-env": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^2.62.0",
    "rollup-plugin-copy": "^3.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "photo",
    "viewer",
    "gallery",
    "light",
    "box",
    "lightbox",
    "react",
    "image",
    "display",
    "masonry",
    "photoswipe"
  ]
}
